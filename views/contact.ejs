            <div class="col-md-6 col-sm-12">
                <h3 class="my-5">Send Us a Message</h3>


                <form action="/contact" method="post">
                    <div class="py-4">
                        <input type="text" class="form-control" id="exampleFormControlInput1" name="fullName"
                            placeholder="Full Name" required>
                    </div>
                    <div class="py-4">
                        <input type="email" class="form-control" id="exampleFormControlInput2" name="emailID"
                            placeholder="E-Mail Address" required minlength="6">
                    </div>
                    <div class="py-4">
                        <input type="text" class="form-control" id="exampleFormControlInput3" name="subject"
                            placeholder="Subject" required maxlength="100" minlength="1">
                    </div>
                    <div class="py-4">
                        <textarea class="form-control" id="exampleFormControlTextarea1" name="message" rows="3"
                            placeholder="Your Message" required maxlength="200"></textarea>
                    </div>


                    <button type="submit" onsubmit="checker()" class="btn btn-danger my-3 rounded"
                        style="font-weight: 400;">Send
                        Message</button>


                </form>
